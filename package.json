{
  "name": "zoro",
  "version": "2.2.1",
  "private": true,
  "engines": {
    "node": ">=18.x.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:format": "yarn prettier \"src/**/*.{js,jsx,ts,tsx,html,json,md}\" public/**/*.html --cache --check",
    "lint:ts": "export NODE_OPTIONS=\"--max-old-space-size=4096\" && eslint . --cache",
    "lint:styles": "stylelint 'src/**/*.{css,scss,ts,tsx,js,jsx}'",
    "typecheck": "tsc",
    "format": "yarn lint:format --write && yarn lint:ts --fix && yarn lint:styles --fix",
    "extract-translation-keys": "i18next",
    "generate-subgraph-types:mainnet": "yarn graphql-codegen --config ./src/config/codegen.ts --project mainnet",
    "generate-subgraph-types:testnet": "yarn graphql-codegen --config ./src/config/codegen.ts --project testnet",
    "generate-subgraph-types": "npm-run-all --parallel generate-subgraph-types:mainnet generate-subgraph-types:testnet",
    "generate-contract-types": "rimraf src/types/contracts && typechain --target=ethers-v5 --out-dir=src/types/contracts 'src/constants/contracts/abis/*.json'",
    "generate-version-file": "genversion --es6 --semi src/constants/version.ts",
    "postinstall": "npm-run-all --parallel generate-contract-types generate-version-file",
    "regression": "reg-suit run"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.13.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/web": "^5.7.1",
    "@infinitywallet/infinity-connector": "^1.0.5",
    "@lottiefiles/react-lottie-player": "^3.4.5",
    "@mui/material": "^5.16.7",
    "@mui/material-nextjs": "^5.16.6",
    "@mui/utils": "^5.16.6",
    "@pancakeswap/sdk": "^3.1.0",
    "@tanstack/react-query": "^5.51.23",
    "@web3-onboard/coinbase": "^2.2.6",
    "@web3-onboard/core": "^2.21.2",
    "@web3-onboard/injected-wallets": "^2.10.9",
    "@web3-onboard/ledger": "^2.5.2",
    "@web3-onboard/react": "^2.8.13",
    "@web3-onboard/trezor": "^2.4.3",
    "@web3-onboard/trust": "^2.0.4",
    "@web3-onboard/walletconnect": "^2.5.0",
    "bignumber.js": "^9.0.0",
    "copy-to-clipboard": "^3.3.1",
    "date-fns": "^2.28.0",
    "ethereum-multicall": "2.15.0",
    "ethers": "^5.7.2",
    "formik": "^2.2.9",
    "i18next": "^21.6.15",
    "lodash": "^4.17.21",
    "next": "14.2.5",
    "noop-ts": "^1.0.3",
    "react": "^18",
    "react-countdown": "^2.3.2",
    "react-dom": "^18",
    "react-i18next": "^11.16.5",
    "react-toastify": "^10.0.5",
    "react-uid": "^2.3.1",
    "recharts": "2.1.9",
    "request": "^2.88.2",
    "sass": "^1.77.8",
    "sharp": "^0.33.4",
    "yup": "^1.0.2",
    "zksync-web3": "^0.14.3"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "0.37.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^8.0.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-toastify": "^4.1.0",
    "@types/recharts": "^1.8.23",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "genversion": "^3.1.1",
    "i18next-parser": "^6.3.0",
    "ignore-loader": "^0.1.2",
    "lint-staged": "^12.3.3",
    "npm": "^9.6.5",
    "npm-run-all2": "^6.0.5",
    "path": "^0.12.7",
    "rimraf": "^4.1.2",
    "stylelint": "^14.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.9.2",
    "typechain": "^8.1.1",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
